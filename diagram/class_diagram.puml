class Bass{

}

class Keyboard {

}

class BookingStatus <<enumeration>> {
    CART
    ORDERED
    CONFIRMED
    CANCELLED
}

class Booking {
    -booking_id : str
    -customer : class
    -room : class
    -time_slot : class
    -equipment_list : list
    -created_at : datetime
    -status : Enum
    -price : float

    +add_equipment() : void
    +remove_equipment(): void
    +change_room() : void
    +change_time_slot() : void

    +calculate_price() : float

    +change_status(new_status: BookingStatus) : void
}

class OrderStatus <<enumeration>> {
    PENDING_PAYMENT
    PAID
    CANCELLED
    REFUNDED
}

class Order {
    -order_id : str
    -customer : class
    -booking_list : list
    -created_at : datetime
    -status : Enum
    -total_price : float
    -coupon : Coupon

    +add_booking(booking: Booking) : void
    +remove_booking(booking: Booking) : void

    +calculate_total() : float
    +apply_coupon(coupon: Coupon) : void

    +change_status(new_status: OrderStatus) : void
}


class PaymentChannel{

}

class QrScan{

}

class CreditCard{

}

class Payment{
    -order : class
    -coupon : class
    -fine : class
    -refund : class
    -payment_channel : class
    -payment_status : class
    -payment_refund()

}

class Transaction{
    -timestamp : datetime
    -order : class
    -type : str
    -check_cancel_rule()

}


class User{
    -user_id
    -user_name
    -password
    -get_transection()

}

