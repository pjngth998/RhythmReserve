class Policy{
    +cancel : void
    +refund : void
    +fine : void
    -check_cancel_rule()

}

class Notification{
    -user : class
    -order : class

}



class Coupon{
    -expired_date : datetime
    -discount : float

}



User <|-- Officer
User <|-- Customer
Customer <|-- Standard
Customer <|-- Premium
Customer <|-- Diamond
Officer <|-- BranchManager
Officer <|-- FrontDesk
Officer <|-- SystemAdmin
Room <|-- SmallRoom
